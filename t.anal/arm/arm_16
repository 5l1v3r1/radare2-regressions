for a in . .. ../.. ; do [ -e $a/tests.sh ] && . $a/tests.sh ; done

NAME="thumb ldr pc-rel analysis"
FILE=malloc://32
CMDS="
e asm.arch=arm
e asm.bits=16
wx dff80000 12000000 34000000
pd 1
"
EXPECT='            0x00000000      dff80000       ldr.w r0, [pc, 0]           ; [0x8:4]=52 ; 8
'
run_test

