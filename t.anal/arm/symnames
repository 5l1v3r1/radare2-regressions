#!/bin/bash

for a in . .. ../.. ; do [ -e $a/tests.sh ] && . $a/tests.sh ; done

NAME="ELF ARM: function names"
BROKEN=
FILE=../../bins/elf/arm_32_flags0
CMDS='
aa @@ sym*
afl~abort
'
EXPECT='0x000102bc  12  1  sym.imp.abort
'
run_test

NAME="ELF ARM: function names 2"
BROKEN=1
FILE=../../bins/elf/arm_32_flags0
CMDS='
aa @@ sym*
afl~weak
'
EXPECT='0x00010304  12  1  sym.call_weak_fn
'
run_test

NAME="ELF ARM: aa"
BROKEN=1
FILE=../../bins/elf/arm_32_flags0
CMDS='
aa
afl~?
'
EXPECT='29
'
run_test

NAME="ELF ARM: af @@ sym.*"
BROKEN=
FILE=../../bins/elf/arm_32_flags0
CMDS='
af @@ sym.*
afl~?
'
EXPECT='29
'
run_test
