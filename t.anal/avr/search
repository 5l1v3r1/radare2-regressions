for a in . .. ../.. ; do [ -e $a/tests.sh ] && . $a/tests.sh ; done

PLUGIN=avr

NAME="avr search"
BROKEN=1
FILE=../../bins/firmware/arduino_avr.bin
CMDS="
aaa
/c call 0x4ec
"
EXPECT='0x000003e2 # 4: call 0x4ec
'
run_test


NAME="avr search"
BROKEN=1
FILE=../../bins/firmware/arduino_avr.bin
CMDS="
aaa
axt 0x4ec
"
EXPECT='C 0x000003e2 call fcn.000004ec
'
run_test



