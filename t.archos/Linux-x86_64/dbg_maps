for a in . .. ../.. ; do [ -e $a/tests.sh ] && . $a/tests.sh ; done

NAME='dbg.maps.count/'
FILE=../../bins/elf/analysis/elf-nx
ARGS=-d
BROKEN=
CMDS='dm~?/
dk 9'
EXPECT='4
'
IGNORE_ERR=1
run_test

NAME='dbg.maps.count'
FILE=../../bins/elf/analysis/elf-nx
ARGS=-d
CMDS='dm~?
dk 9'
EXPECT='6
'
IGNORE_ERR=1
run_test

NAME='dbg.modules.count'
FILE=../../bins/elf/analysis/elf-nx
ARGS=-d
BROKEN=
CMDS='dmm~?'
EXPECT='2
'
IGNORE_ERR=1
run_test

NAME='dbg.curmod.count'
FILE=../../bins/elf/analysis/elf-nx
ARGS=-d
BROKEN=
CMDS='dmm.~?'
EXPECT='1
'
IGNORE_ERR=1
run_test
