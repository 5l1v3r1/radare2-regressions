NAME='search'
FILE=../../bins/elf/x86-helloworld-gcc
CMDS='/ \x90\x06
'
EXPECT_ERR='Searching 2 bytes from 0x08048000 to 0x0804a59c: 90 06 
hits: 2
'
EXPECT='0x08048b4c hit0_0 "
0x08049b4c hit0_1 "
'
run_test

NAME='flags spaces'
FILE=../../bins/elf/x86-helloworld-gcc
CMDS='fs
'
EXPECT='00   strings
02   symbols
04   relocs
06 * sections
'
run_test

NAME='flags spaces after analysis'
FILE=../../bins/elf/x86-helloworld-gcc
CMDS='aa;fs
'
EXPECT='00   strings
02   symbols
04   relocs
06   sections
08 * functions
'
run_test

NAME='pb 4'
FILE=../../bins/elf/x86-helloworld-gcc
CMDS='pb 4
'
EXPECT='0x08048300 0011.0001  1110.1101  0101.1110  1000.1001  0x31ed5e89  1.^.
'
run_test

NAME='pxW 10'
