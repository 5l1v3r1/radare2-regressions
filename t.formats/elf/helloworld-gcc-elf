NAME='pu 10'
FILE=../../bins/elf/x86-helloworld-gcc
CMDS='pu 10
'
EXPECT='%31%ed%5e%89%e1%83%e4%f0%50%54
'
run_test

NAME='pdn 10'
FILE=../../bins/elf/x86-helloworld-gcc
CMDS='e scr.color = 0
pdn 10
'
EXPECT='0x08048300             31ed  xor ebp, ebp
0x08048302               5e  pop esi
0x08048303             89e1  mov ecx, esp
0x08048305           83e4f0  and esp, 0xfffffff0
0x08048308               50  push eax
0x08048309               54  push esp
'
run_test

NAME='pi 12'
FILE=../../bins/elf/x86-helloworld-gcc
CMDS='pi 12
'
EXPECT='xor ebp, ebp
pop esi
mov ecx, esp
and esp, 0xfffffff0
push eax
push esp
push edx
push 0x8048490
push 0x8048420
push ecx
push esi
push 0x8048400
'
run_test


