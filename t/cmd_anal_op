#!/bin/sh

# GPL 3+ - Copyright (C) 2013  pancake

[ -e tests.sh ] && . ./tests.sh || . ../tests.sh

NAME='af x86-32'
FILE=malloc://16
ARGS=
CMDS='
e asm.arch=x86
e asm.bits=32
wx 9090
ao 1~?
ao 1
p8 1
ao 1~?
'
EXPECT='9
opcode: nop
addr: 0x00000000
bytes: 90
size: 1
type: 8 (nop)
esil: ,
stack: null
cond: 0
family: 0
90
9
'
run_test

NAME='af x86-64'
FILE=malloc://16
ARGS=
CMDS='
e asm.arch=x86
e asm.bits=64
wx 662e0f1f840000000000
ao 1~size[1]
'
EXPECT='10
'
run_test

