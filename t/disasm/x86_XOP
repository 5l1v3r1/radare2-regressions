#!/bin/sh
for a in . .. ../.. ; do [ -e $a/tests.sh ] && . $a/tests.sh ; done
for ARCH in x86 x86.olly x86.cs; do


# -------------------------------
# VPMADCSSWD XMM0, XMM0, XMM0, XMM0 instruction
BROKEN=1
NAME="x86 XOP: [VPMADCSSWD XMM0, XMM0, XMM0, XMM0]"
FILE=malloc://32
CMDS="
e asm.arch=${ARCH}
e asm.bits=32
wx 8fe878a6c000
pi 1
"
EXPECT="vpmadcsswd xmm0, xmm0, xmm0, xmm0
"
run_test

# -------------------------------
