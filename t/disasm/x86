#!/bin/sh
for a in . .. ../.. ; do [ -e $a/tests.sh ] && . $a/tests.sh; done
for ARCH in x86 x86.olly x86.cs; do


# -------------------------------
# CWDE instruction
NAME="${ARCH}: [CWDE]"
FILE=malloc://32
CMDS="
e asm.arch=${ARCH}
e asm.bits=32
wx 98
pi 1
"
EXPECT="cwde
"
run_test


# -------------------------------
# CBW instruction
NAME="${ARCH}: [CBW]"
FILE=malloc://32
