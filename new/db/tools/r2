NAME=r2 -
FILE=-
CMDS=<<EXPECT
e file.path
EXPECT=<<RUN

RUN

NAME=r2 -
FILE=-
CMDS=<<EXPECT
e file.lastpath
EXPECT=<<RUN

RUN

NAME=r2 --
FILE=--
CMDS=<<EXPECT
e file.path
EXPECT=<<RUN

RUN

NAME=r2 --
FILE=../bins/elf/bomb
CMDS=<<EXPECT
r2 -s main -qc "s" ../bins/elf/bomb
EXPECT=<<RUN
0x400da0
RUN

NAME=seek undo/redo
FILE=-
CMDS=<<EXPECT
wx 01
s 1
s-
s+
pb 8
EXPECT=<<RUN
00000000
RUN

NAME=seek undo/redo
FILE=-
CMDS=<<EXPECT
wx 01; s 1; s-; s+; pb 8
EXPECT=<<RUN
00000000
RUN
