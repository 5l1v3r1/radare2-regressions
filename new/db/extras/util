NAME=Sort Params
FILE=-
EXPECT=<<EOF
eleven
five
five
great
one
one
radare
EOF
CMDS=<<EOF
sort ../bins/other/test_file
EOF
RUN

NAME=Missing Sort Params
FILE=-
EXPECT=<<EOF
EOF
CMDS=<<EOF
sort 
EOF
RUN

NAME=Uniq command
FILE=-
EXPECT=<<EOF
one
five
eleven
radare
great
EOF
CMDS=<<EOF
uniq ../bins/other/test_file
EOF
RUN

NAME=Missing Uniq Params
FILE=-
EXPECT=<<EOF
EOF
CMDS=<<EOF
uniq 
EOF
RUN

NAME=Join command
FILE=-
EXPECT=<<EOF
one one
five five
eleven eleven
radare radare
great great
five five
one one
EOF
CMDS=<<EOF
join ../bins/other/test_file ../bins/other/test_file
EOF
RUN

NAME=Missing join Params
FILE=-
EXPECT=<<EOF
EOF
CMDS=<<EOF
join ../bins/other/test_file
EOF
RUN